{
  "name": "onstarjs2",
  "version": "2.12.1",
  "description": "Unofficial package for making OnStar API requests",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rimraf dist && rollup -c -w",
    "lint": "prettier --write .",
    "prepublishOnly": "pnpm run build",
    "release": "commit-and-tag-version --sign",
    "test": "pnpm test:unit && pnpm test:functional",
    "test:unit": "jest test/unit",
    "test:coverage": "jest test/unit --coverage",
    "test:functional": "jest test/functional",
    "test:keys": "jest test/functional -t 'Unupgraded Command Successful'",
    "test:auth": "jest test/auth/testAuth",
    "test:reauth": "jest test/auth/testAuth -t 'should successfully reauthenticate'",
    "prepare": "husky install",
    "postinstall": "patchright install chromium",
    "setup:env": "node scripts/setup-env.js",
    "setup:env:bash": "chmod +x scripts/setup-env.sh && ./scripts/setup-env.sh",
    "setup:interactive": "node scripts/credential-manager.js",
    "run:request-service": "pnpm run build && node scripts/request-service-cli.mjs",
    "run:get-vehicles": "pnpm run build && node scripts/get-vehicles.mjs",
    "run:get-diagnostics": "pnpm run build && node scripts/get-diagnostics.mjs",
    "run:get-tire-pressure": "pnpm run build && node scripts/get-tire-pressure.mjs"
  },
  "lint-staged": {
    "*.{js,ts,md,yml}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.18.3",
  "standard-version": {
    "scripts": {
      "prerelease": "pnpm test:unit && pnpm build"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigThunderSR/OnStarJS.git"
  },
  "keywords": [
    "onstar",
    "chevrolet",
    "chevy",
    "gm",
    "gmc",
    "buick",
    "cadillac"
  ],
  "author": "Ruben Medina <dev@rubenmedina.com> (https://rubenmedina.com), updated by @BigThunderSR (https://github.com/BigThunderSR) and others.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BigThunderSR/OnStarJS/issues"
  },
  "homepage": "https://github.com/BigThunderSR/OnStarJS#readme",
  "dependencies": {
    "axios": "^1.12.2",
    "chromium-bidi": "^10.6.0",
    "http-cookie-agent": "^7.0.2",
    "jsonwebtoken": "^9.0.2",
    "openid-client": "^5.7.1",
    "patchright": "^1.56.1",
    "totp-generator": "^2.0.0",
    "tough-cookie": "^5.1.2",
    "uuid": "^11.1.0",
    "xvfb": "^0.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^11.0.0",
    "commit-and-tag-version": "^12.6.0",
    "dotenv": "^17.2.3",
    "husky": "9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.5",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.5",
    "ts-jest": "^29.4.5",
    "ts-mockito": "^2.6.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
